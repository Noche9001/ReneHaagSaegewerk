{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build --configuration production && ng build --configuration production --ssr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/server/server.mjs"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.1.2",
    "@angular/compiler": "20.1.2",
    "@angular/core": "20.1.2",
    "@angular/forms": "20.1.2",
    "@angular/platform-browser": "20.1.2",
    "@angular/platform-server": "20.1.2",
    "@angular/router": "20.1.2",
    "@angular/ssr": "20.1.2",
    "@tailwindcss/postcss": "^4.1.11",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.2.4",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "20.1.2",
    "@angular/cli": "20.0.5",
    "@angular/compiler-cli": "20.1.2",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-coverage": "~2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.2"
  }
}
